#: plasma_workspace.notifyrc:3
msgctxt "Comment"
msgid "Plasma Workspace"
msgstr "Espai de treball del Plasma"

#: plasma_workspace.notifyrc:54
msgctxt "Name"
msgid "Trash: Emptied"
msgstr "Paperera: s'ha buidat"

#: plasma_workspace.notifyrc:103
msgctxt "Comment"
msgid "The trash has been emptied"
msgstr "S'ha buidat la paperera"

#: plasma_workspace.notifyrc:156
msgctxt "Name"
msgid "Textcompletion: Rotation"
msgstr "Compleció del text: rotació"

#: plasma_workspace.notifyrc:203
msgctxt "Comment"
msgid "The end of the list of matches has been reached"
msgstr "S'ha arribat al final de la llista de coincidències"

#: plasma_workspace.notifyrc:255
msgctxt "Name"
msgid "Textcompletion: No Match"
msgstr "Compleció del text: sense coincidència"

#: plasma_workspace.notifyrc:303
msgctxt "Comment"
msgid "No matching completion was found"
msgstr "No s'ha trobat cap compleció coincident"

#: plasma_workspace.notifyrc:355
msgctxt "Name"
msgid "Textcompletion: Partial Match"
msgstr "Compleció del text: coincidència parcial"

#: plasma_workspace.notifyrc:403
msgctxt "Comment"
msgid "There is more than one possible match"
msgstr "Hi ha més d'una possible coincidència"

#: plasma_workspace.notifyrc:455
msgctxt "Name"
msgid "Fatal Error"
msgstr "Error fatal"

#: plasma_workspace.notifyrc:504
msgctxt "Comment"
msgid "There was a serious error causing the program to exit"
msgstr "S'ha produït un error seriós que ha provocat l'eixida del programa"

#: plasma_workspace.notifyrc:557
msgctxt "Name"
msgid "Notification"
msgstr "Notificació"

#: plasma_workspace.notifyrc:606
msgctxt "Comment"
msgid "Something special happened in the program"
msgstr "Alguna cosa especial ha passat en el programa"

#: plasma_workspace.notifyrc:658
msgctxt "Name"
msgid "Warning"
msgstr "Avís"

#: plasma_workspace.notifyrc:707
msgctxt "Comment"
msgid "There was an error in the program which may cause problems"
msgstr "S'ha produït un error en el programa que pot provocar problemes"

#: plasma_workspace.notifyrc:759
msgctxt "Name"
msgid "Catastrophe"
msgstr "Catàstrofe"

#: plasma_workspace.notifyrc:807
msgctxt "Comment"
msgid "A very serious error occurred, at least causing the program to exit"
msgstr ""
"S'ha produït un error molt seriós, com a mínim ha provocat l'eixida del "
"programa"

#: plasma_workspace.notifyrc:859
msgctxt "Name"
msgid "Application Crash"
msgstr "Fallada d'aplicació"

#: plasma_workspace.notifyrc:905
msgctxt "Comment"
msgid "An application just closed unexpectedly"
msgstr "Una aplicació s'ha tancat inesperadament"

#: plasma_workspace.notifyrc:953
msgctxt "Name"
msgid "Login"
msgstr "Entrada"

#: plasma_workspace.notifyrc:1002
msgctxt "Comment"
msgid "A new session is starting"
msgstr "S'està iniciat una sessió nova"

#: plasma_workspace.notifyrc:1054
msgctxt "Name"
msgid "Logout"
msgstr "Eixida"

#: plasma_workspace.notifyrc:1103
msgctxt "Comment"
msgid "Session is exiting"
msgstr "La sessió està finalitzant"

#: plasma_workspace.notifyrc:1156
msgctxt "Name"
msgid "Logout Canceled"
msgstr "S'ha cancel·lat l'eixida"

#: plasma_workspace.notifyrc:1205
msgctxt "Comment"
msgid "Plasma logout was canceled"
msgstr "S'ha cancel·lat l'eixida del Plasma"

#: plasma_workspace.notifyrc:1257
msgctxt "Name"
msgid "Print Error"
msgstr "Error en imprimir"

#: plasma_workspace.notifyrc:1306
msgctxt "Comment"
msgid "A print error has occurred"
msgstr "S'ha produït un error en imprimir"

#: plasma_workspace.notifyrc:1358
msgctxt "Name"
msgid "Information Message"
msgstr "Missatge informatiu"

#: plasma_workspace.notifyrc:1407
msgctxt "Comment"
msgid "An information message is being shown"
msgstr "Es mostra un missatge d'informació"

#: plasma_workspace.notifyrc:1459
msgctxt "Name"
msgid "Warning Message"
msgstr "Missatge d'avís"

#: plasma_workspace.notifyrc:1508
msgctxt "Comment"
msgid "A warning message is being shown"
msgstr "Es mostra un missatge d'avís"

#: plasma_workspace.notifyrc:1560
msgctxt "Name"
msgid "Critical Message"
msgstr "Missatge crític"

#: plasma_workspace.notifyrc:1609
msgctxt "Comment"
msgid "A critical message is being shown"
msgstr "Es mostra un missatge crític"

#: plasma_workspace.notifyrc:1661
msgctxt "Name"
msgid "Question"
msgstr "Pregunta"

#: plasma_workspace.notifyrc:1710
msgctxt "Comment"
msgid "A question is being asked"
msgstr "Es pregunta una qüestió"

#: plasma_workspace.notifyrc:1763
msgctxt "Name"
msgid "Beep"
msgstr "Timbre"

#: plasma_workspace.notifyrc:1812
msgctxt "Comment"
msgid "Sound bell"
msgstr "So de campana"

#: plasma_workspace.notifyrc:1864
msgctxt "Name"
msgid "Widget failed to install"
msgstr "Ha fallat en instal·lar el giny («widget»)"

#: plasma_workspace.notifyrc:1910
msgctxt "Comment"
msgid "A widget could not be installed"
msgstr "No s'ha pogut instal·lar un giny («widget»)"

#: plasma_workspace.notifyrc:1959
msgctxt "Name"
msgid "Widget deleted"
msgstr "Giny («Widget») suprimit"

#: plasma_workspace.notifyrc:2006
msgctxt "Comment"
msgid "A widget has been deleted"
msgstr "S'ha suprimit un giny («widget»)"
