#: ktorrent/ktorrent.notifyrc:4
msgctxt "Comment"
msgid "KTorrent"
msgstr "KTorrent"

#: ktorrent/ktorrent.notifyrc:63
msgctxt "Name"
msgid "Torrent silently opened"
msgstr "El torrent s'ha obert en silenci"

#: ktorrent/ktorrent.notifyrc:107
msgctxt "Name"
msgid "Torrent stopped by error"
msgstr "El torrent s'ha aturat per un error"

#: ktorrent/ktorrent.notifyrc:160
msgctxt "Name"
msgid "Torrent has finished downloading"
msgstr "El torrent ha finalitzat la baixada"

#: ktorrent/ktorrent.notifyrc:213
msgctxt "Name"
msgid "Maximum share ratio reached"
msgstr "S'ha assolit la velocitat màxima de compartició"

#: ktorrent/ktorrent.notifyrc:266
msgctxt "Name"
msgid "Maximum seed time reached"
msgstr "S'ha assolit el temps màxim de sembrat"

#: ktorrent/ktorrent.notifyrc:319
msgctxt "Name"
msgid "Disk space is running low"
msgstr "Queda poc espai al disc"

#: ktorrent/ktorrent.notifyrc:373
msgctxt "Name"
msgid "Corrupted data has been found"
msgstr "S'han trobat dades corruptes"

#: ktorrent/ktorrent.notifyrc:428
msgctxt "Name"
msgid "Torrent cannot be enqueued"
msgstr "No s'ha pogut posar el torrent a la cua"

#: ktorrent/ktorrent.notifyrc:480
msgctxt "Name"
msgid "Torrent cannot be started"
msgstr "No s'ha pogut engegar el torrent"

#: ktorrent/ktorrent.notifyrc:533
msgctxt "Name"
msgid "Torrent cannot be loaded silently"
msgstr "No s'ha pogut carregar silenciosament el torrent"

#: ktorrent/ktorrent.notifyrc:586
msgctxt "Name"
msgid "DHT is not enabled"
msgstr "El DHT no està actiu"

#: ktorrent/ktorrent.notifyrc:637
msgctxt "Name"
msgid "Event generated by plugin"
msgstr "Esdeveniment generat pel connector"

#: ktorrent/ktorrent.notifyrc:684
msgctxt "Name"
msgid "Magnet link download started"
msgstr "S'ha iniciat la baixada de l'enllaç «magnet»"

#: ktorrent/ktorrent.notifyrc:729
msgctxt "Name"
msgid "Magnet link copied to clipboard"
msgstr "L'enllaç magnet s'ha copiat al porta-retalls"

#: ktorrent/org.kde.ktorrent.desktop:2
msgctxt "Name"
msgid "KTorrent"
msgstr "KTorrent"

#: ktorrent/org.kde.ktorrent.desktop:62
msgctxt "GenericName"
msgid "BitTorrent Client"
msgstr "Client de BitTorrent"

#: ktorrent/org.kde.ktorrent.desktop:128
msgctxt "Comment"
msgid "A BitTorrent program by KDE"
msgstr "Un programa de BitTorrent, creat per la comunitat KDE"

#: plugins/bwscheduler/ktorrent_bwscheduler.desktop:2
msgctxt "Name"
msgid "Scheduler"
msgstr "Planificador"

#: plugins/bwscheduler/ktorrent_bwscheduler.desktop:49
msgctxt "Comment"
msgid "Schedule upload and download limits over a period of a week"
msgstr "Planifica els límits de pujada i baixada durant un període setmanal"

#: plugins/downloadorder/ktorrent_downloadorder.desktop:2
msgctxt "Name"
msgid "Download Order"
msgstr "Ordre de baixada"

#: plugins/downloadorder/ktorrent_downloadorder.desktop:54
msgctxt "Comment"
msgid "Specify the download order of a multi-file torrent"
msgstr "Especifica l'ordre de baixada d'un torrent amb múltiples fitxers"

#: plugins/infowidget/ktorrent_infowidget.desktop:2
msgctxt "Name"
msgid "Information Widget"
msgstr "Giny d'informació"

#: plugins/infowidget/ktorrent_infowidget.desktop:54
msgctxt "Comment"
msgid "Displays general information about a torrent in several tabs"
msgstr "Mostra informació general quant a un torrent en diverses pestanyes"

#: plugins/ipfilter/ktorrent_ipfilter.desktop:2
msgctxt "Name"
msgid "IP Filter"
msgstr "Filtre d'IP"

#: plugins/ipfilter/ktorrent_ipfilter.desktop:55
msgctxt "Comment"
msgid "Filter IP addresses through a blocklist"
msgstr "Filtra les adreces IP a través d'una llista de bloqueig"

#: plugins/logviewer/ktorrent_logviewer.desktop:2
msgctxt "Name"
msgid "Log Viewer"
msgstr "Visor del registre"

#: plugins/logviewer/ktorrent_logviewer.desktop:56
msgctxt "Comment"
msgid "Displays the logging output"
msgstr "Mostra l'eixida del registre"

#: plugins/magnetgenerator/ktorrent_magnetgenerator.desktop:2
msgctxt "Name"
msgid "Magnet Generator"
msgstr "Generador de magnet"

#: plugins/magnetgenerator/ktorrent_magnetgenerator.desktop:47
msgctxt "Comment"
msgid "Generates magnet links"
msgstr "Genera enllaços magnet"

#: plugins/mediaplayer/ktorrent_mediaplayer.desktop:2
msgctxt "Name"
msgid "Media Player"
msgstr "Reproductor multimèdia"

#: plugins/mediaplayer/ktorrent_mediaplayer.desktop:57
msgctxt "Comment"
msgid "Phonon-based media player"
msgstr "Reproductor de suports basat en el Phonon"

#: plugins/scanfolder/ktorrent_scanfolder.desktop:2
msgctxt "Name"
msgid "Scan Folder"
msgstr "Explora les carpetes"

#: plugins/scanfolder/ktorrent_scanfolder.desktop:53
msgctxt "Comment"
msgid "Scan folders for torrent files and load them"
msgstr "Explora les carpetes cercant fitxers torrent i els carrega"

#: plugins/scanforlostfiles/ktorrent_scanforlostfiles.desktop:2
msgctxt "Name"
msgid "Scan for lost files"
msgstr "Explora els fitxers perduts"

#: plugins/scanforlostfiles/ktorrent_scanforlostfiles.desktop:25
msgctxt "Comment"
msgid "Display files in the selected folder that don't belong to any torrent"
msgstr ""
"Mostra els fitxers de la carpeta seleccionada que no pertanyen a cap torrent"

#: plugins/search/ktorrent_search.desktop:2
msgctxt "Name"
msgid "Search"
msgstr "Cerca"

#: plugins/search/ktorrent_search.desktop:56
msgctxt "Comment"
msgid "Search for torrents"
msgstr "Cerca de torrent"

#: plugins/shutdown/ktorrent_shutdown.desktop:2
msgctxt "Name"
msgid "Shutdown"
msgstr "Aturada"

#: plugins/shutdown/ktorrent_shutdown.desktop:57
msgctxt "Comment"
msgid "Allows you to shutdown your computer when torrents finish"
msgstr "Permet aturar l'ordinador en finalitzar els torrent"

#: plugins/stats/ktorrent_stats.desktop:3
msgctxt "Name"
msgid "Statistics"
msgstr "Estadístiques"

#: plugins/stats/ktorrent_stats.desktop:58
msgctxt "Comment"
msgid "Shows statistics about torrents in several graphs"
msgstr "Mostra les estadístiques sobre els torrent en diverses gràfiques"

#: plugins/syndication/ktorrent_syndication.desktop:2
msgctxt "Name"
msgid "Syndication"
msgstr "Sindicació"

#: plugins/syndication/ktorrent_syndication.desktop:51
msgctxt "Comment"
msgid "Syndication plugin for KTorrent, supporting RSS and Atom feeds"
msgstr "Connector de sindicació per al KTorrent, permet canals RSS i Atom"

#: plugins/upnp/ktorrent_upnp.desktop:2
msgctxt "Name"
msgid "UPnP"
msgstr "UPnP"

#: plugins/upnp/ktorrent_upnp.desktop:55
msgctxt "Comment"
msgid "Forward ports using UPnP"
msgstr "Rediregeix els ports usant UPnP"

#: plugins/zeroconf/ktorrent_zeroconf.desktop:2
msgctxt "Name"
msgid "Zeroconf"
msgstr "Zeroconf"

#: plugins/zeroconf/ktorrent_zeroconf.desktop:54
msgctxt "Comment"
msgid "Discover peers on the local network using the Zeroconf protocol"
msgstr "Descobreix els parells a la xarxa local usant el protocol Zeroconf"

